{
    "name": "zingazzi/italian-date-time",
    "description": "Italian datepicker for Laravel Nova.",
    "keywords": [
        "laravel",
        "nova",
        "datepicker",
        "date",
        "picker",
        "italian",
        "localisation"
    ],
    "authors": [
        {
           "name": "Marco Zingoni",
           "email": "marco.zingoni@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "illuminate/support": "^5.6",
        "laravel/nova": "*"
    },
    "autoload": {
        "psr-4": {
            "Zingazzi\\ItalianDateTime\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zingazzi\\ItalianDateTime\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
